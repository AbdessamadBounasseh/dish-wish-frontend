<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="-35dp"
    tools:context=".Authentification.terms">
    <LinearLayout
        android:id="@+id/bars_top_ba"
        android:layout_width="375dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="8dp"
        android:longClickable="false"
        >
        <ImageView
            android:id="@+id/icon_24_bac"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:src="@drawable/ic_back"

            />

        <TextView
            android:id="@+id/sign_in"
            android:layout_width="187dp"
            android:layout_height="wrap_content"
            android:letterSpacing="-0.03"
            android:lineSpacingExtra="8sp"
            android:text="Terms  &amp; Conditions"

            android:textSize="16sp"
            android:layout_marginLeft="63dp"
            android:textColor="#010F07"
            android:gravity="center_horizontal|center_vertical"/>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/group_103"
        android:layout_width="345dp"
        android:layout_height="535dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="80dp"

        >

        <TextView
            android:id="@+id/general_1_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="37dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="29dp"
            android:text="@string/general_1_1"
            android:lineSpacingExtra="13sp"
            android:translationY="-5.24sp"
            android:gravity="top"
            android:textSize="14sp"
            android:lineHeight="25sp"
            android:fontFamily="@font/interlight"
            android:background="@drawable/rounded_corners"


            />
    </LinearLayout>

    <Button
        android:layout_width="283dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_edittext"
        android:backgroundTint="#EEA734"
        android:text="START"
        android:textColor="#FFFFFFFF"
        android:textSize="14sp"
        app:backgroundTint="@null"
        android:id="@+id/st"
        android:layout_marginTop="630dp"
        android:layout_marginLeft="95dp"

        />

</RelativeLayout>

